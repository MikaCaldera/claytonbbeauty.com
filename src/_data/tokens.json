{
  "colors": {
    "themeColors": {
      "primary": "#040F3D",
      "secondary": "#0FFFFF",
      "success": "#FF1FFF",
      "info": "#59636E",
      "warning": "#ffc107",
      "danger": "#dc3545",
      "light": "#f8f9fa",
      "dark": "#0A181A"
    },
    "bodyColors": {
      "bodyBg": "#ffffff",
      "body-color": "#040F3D",
      "sidebar-bg": "#f8f9fa",
      "sidebar-color": "#404040",
      "headings-color": "#040F3D",
      "footer-bg": "#0FFFFF",
      "footer-color": "#040F3D"
    },
    "headerColors": {
      "header-bg": "#59636E",
      "navbar-bg": "#EFEFEF"
    }
  },
  "global-settings": {
    "enable-rounded": false,
    "enable-shadows": "true",
    "enable-gradients": false,
    "enable-responsive-font-sizes": "true"
  },
  "fonts": {
    "h6-font-size": 1.25,
    "headingsFontName": "Merriweather",
    "baseFontName": "Montserrat",
    "googleFonts": [
      "\"Open+Sans:wght@400;600;700;800\"",
      "\"Recursive:wght@400;700\""
    ],
    "h1-font-size": 4,
    "h2-font-size": 3.5,
    "h3-font-size": 2.5,
    "h4-font-size": 2,
    "h5-font-size": 1.75
  },
  "spacing": {
    "logo-height": 7,
    "section-padding": "2",
    "section-margins": "3",
    "sidebar-padding": "2",
    "content-cols": "6",
    "sidebar-cols": 5
  }
}